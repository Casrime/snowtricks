<div class="modal fade" id="{{ htmlName }}-{{ id }}" tabindex="-1" aria-labelledby="{{ htmlName }}-{{ id }}-Label" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="{{ htmlName }}-{{ id }}-Label">Delete {{ name }} with id {{ id }} ?</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <p>Are you sure you want to delete this {{ htmlName }}: {{ name }} ?</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                {{ include('trick/_delete_form.html.twig', {
                    content: 'Delete',
                }) }}
            </div>
        </div>
    </div>
</div>
